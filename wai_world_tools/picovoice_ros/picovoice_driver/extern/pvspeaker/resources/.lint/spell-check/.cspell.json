{
  "language": "en",
  "dictionaries": [
    "dict"
  ],
  "dictionaryDefinitions": [
    {
      "name": "dict",
      "path": "./dict.txt",
      "addWords": true
    }
  ],
  "ignorePaths": [
    "**/CMakeLists.txt",
    "**/Preload.CMake",

    // submodules
    "../../project/src/miniaudio",

    // binaries
    "**/*.a",
    "**/*.dll",
    "**/*.dylib",
    "**/*.mp3",
    "**/*.node",
    "**/*.ppn",
    "**/*.so",
    "**/*.wasm",
    "**/*.wav",
  ]
}